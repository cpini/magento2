authorityKeyIdentifier=keyid,issuer
basicConstraints=CA:FALSE
extendedKeyUsage=serverAuth,clientAuth
keyUsage = digitalSignature, nonRepudiation, keyEncipherment, dataEncipherment
subjectAltName = @alt_names
[alt_names]
DNS.1 = mage.xyz # Be sure to include the domain name here because Common Name is not so commonly honoured by itself
DNS.2 = www.mage.xyz # Optionally, add additional domains (I've added a subdomain here)
DNS.3 = static.mage.xyz
